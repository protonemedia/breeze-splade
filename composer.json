{
    "name": "protonemedia/laravel-splade-breeze",
    "description": "Minimal Laravel authentication scaffolding with Laravel Splade.",
    "keywords": [
        "laravel",
        "auth",
        "splade",
        "laravel-splade"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/protonemedia/laravel-splade-breeze/issues",
        "source": "https://github.com/protonemedia/laravel-splade-breeze"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.48.3|^11.0",
        "illuminate/filesystem": "^10.48.3|^11.0",
        "illuminate/support": "^10.48.3|^11.0",
        "illuminate/validation": "^10.48.3|^11.0",
        "protonemedia/laravel-splade": "^1.4.18"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Breeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.1"
    }
}